        rdfs:comment "BE, CH, CZ, DE, FI, FR, NO,  IE, NL, UK, PL, SE"^^xsd:string,
        rdfs:label "country code (two-letter)"^^xsd:string
    
    Domain: 
        Station
    
    Range: 
        {"BE"^^xsd:string , "CH"^^xsd:string , "CZ"^^xsd:string , "DE"^^xsd:string , "FI"^^xsd:string , "FR"^^xsd:string , "IE"^^xsd:string , "IT"^^xsd:string , "NL"^^xsd:string , "NO"^^xsd:string , "PL"^^xsd:string , "SE"^^xsd:string , "UK"^^xsd:string}
    
    Characteristics: 
        Functional
    
    
DataProperty: isAlreadyOperational

    Annotations: 
        rdfs:label "already operational (true/false)"^^xsd:string
    
