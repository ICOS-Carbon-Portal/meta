DataProperty: hasLon

    Annotations: 
        rdfs:label "decimal longitude"^^xsd:string
    
    Domain: 
        Station
    
    Range: 
        xsd:double[>= "-180.0"^^xsd:double , <= "180.0"^^xsd:double]
    
    Characteristics: 
        Functional
    
    
DataProperty: hasStationClass

    Annotations: 
        rdfs:comment "1, 2 or 3"^^xsd:string,
        rdfs:label "ICOS station class"^^xsd:string
    
    Domain: 
        Station
    
    Range: 
        {1 , 2 , 3}
    
    Characteristics: 
        Functional
    
    
DataProperty: hasLat

    Annotations: 
        rdfs:label "decimal latitude"^^xsd:string
    
