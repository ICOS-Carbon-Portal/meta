DataProperty: hasPreIcosMeasurements

    Annotations: 
        rdfs:label "has pre-ICOS measurements (true/false)"^^xsd:string
    
    Domain: 
        Station
    
    Range: 
        xsd:boolean
    
    Characteristics: 
        Functional
    
    
DataProperty: hasStationKind

    Annotations: 
        rdfs:label "kind of station (TT, aircraft, atm, ground, eco, ocean)"^^xsd:string
    
    Domain: 
        Station
    
    Range: 
        {"TT"^^xsd:string , "aircraft"^^xsd:string , "atm"^^xsd:string , "eco"^^xsd:string , "ground"^^xsd:string , "ocean"^^xsd:string}
    
    Characteristics: 
        Functional
    
    
DataProperty: hasSiteType

    Annotations: 
        rdfs:label "site type"^^xsd:string
    
    Domain: 
        Station
    
    Range: 
        xsd:string
    
    Characteristics: 
        Functional
    
    
DataProperty: hasPiName

    Annotations: 
        rdfs:label "principal investigator name"^^xsd:string
    
    Domain: 
        Station
    
    Range: 
        xsd:string
    
    Characteristics: 
        Functional
    
    
DataProperty: hasPiEmail

    Annotations: 
        rdfs:label "principal investigator email"^^xsd:string
    
