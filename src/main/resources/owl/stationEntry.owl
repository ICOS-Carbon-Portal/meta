    <!-- http://meta.icos-cp.eu/ontologies/stationentry/hasPhone -->

    <owl:DatatypeProperty rdf:about="http://meta.icos-cp.eu/ontologies/stationentry/hasPhone">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label rdf:datatype="&xsd;string">phone number (international format)</rdfs:label>
        <rdfs:comment rdf:datatype="&xsd;string">e.g. +46 46 22 29173</rdfs:comment>
        <rdfs:domain rdf:resource="http://meta.icos-cp.eu/ontologies/stationentry/PI"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:onDatatype rdf:resource="&xsd;string"/>
                <owl:withRestrictions rdf:parseType="Collection">
                    <rdf:Description>
                        <xsd:pattern>^\+[\d\s]{8,}$</xsd:pattern>
                    </rdf:Description>
                </owl:withRestrictions>
            </rdfs:Datatype>
        </rdfs:range>
    </owl:DatatypeProperty>
    


