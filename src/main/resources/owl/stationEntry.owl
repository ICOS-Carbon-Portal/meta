        Station
    
    Range: 
        xsd:boolean
    
    Characteristics: 
        Functional
    
    
Class: ES

    Annotations: 
        rdfs:label "Ecosystem Station"^^xsd:string
    
    SubClassOf: 
        hasLon some rdfs:Literal,
        hasLat some rdfs:Literal,
        Station
    
    
Class: Station

    SubClassOf: 
        hasPiEmail some rdfs:Literal,
        hasLongName some rdfs:Literal,
        hasPiName some rdfs:Literal,
        hasShortName some rdfs:Literal
    
    
Class: AS

    Annotations: 
        rdfs:label "Atmospheric Station"^^xsd:string
    
    SubClassOf: 
        hasLat some rdfs:Literal,
        Station,
        hasLon some rdfs:Literal
    
    
Class: OS

    Annotations: 
        rdfs:label "Ocean Station"^^xsd:string
    
    SubClassOf: 
        Station
    
    
